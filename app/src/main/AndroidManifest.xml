<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitUp">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Levelexercise"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FlexibilityLevel"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StrengthLevel"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Cardiolevel"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CardioLow"
            android:parentActivityName=".Cardiolevel" />
        <activity android:name=".CardioLowActivity"
            android:parentActivityName=".CardioLow"></activity>
        <activity
            android:name=".CardioMedium"
            android:parentActivityName=".Cardiolevel" />
        <activity
            android:name=".CardioHigh"
            android:parentActivityName=".Cardiolevel" />
        <activity
            android:name=".FlexHigh"
            android:parentActivityName=".FlexibilityLevel" />
        <activity
            android:name=".FlexMedium"
            android:parentActivityName=".FlexibilityLevel" />
        <activity
            android:name=".FlexLow"
            android:parentActivityName=".FlexibilityLevel" />
        <activity
            android:name=".StrHigh"
            android:parentActivityName=".StrengthLevel" />
        <activity
            android:name=".StrMedium"
            android:parentActivityName=".StrengthLevel" />
        <activity
            android:name=".StrLow"
            android:parentActivityName=".StrengthLevel" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>